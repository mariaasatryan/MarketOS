# -----------------------------------
# Wildberries API
# -----------------------------------
# Get your API token from Wildberries Seller Portal
# https://seller.wildberries.ru/
WB_API_TOKEN=your_wildberries_token_here

# -----------------------------------
# Ozon API
# -----------------------------------
# Get Client ID and API Key from Ozon Seller Portal
# https://seller.ozon.ru/
OZON_API_CLIENT_ID=your_ozon_client_id_here
OZON_API_KEY=your_ozon_api_key_here

# -----------------------------------
# Yandex Market API
# -----------------------------------
# Get OAuth token from Yandex Market Partner Portal
# https://partner.market.yandex.ru/
YM_API_TOKEN=your_yandex_market_token_here
YM_CAMPAIGN_ID=your_campaign_id_here

# -----------------------------------
# Google OAuth & Calendar
# -----------------------------------
# Create OAuth 2.0 credentials in Google Cloud Console
# https://console.cloud.google.com/apis/credentials
VITE_GOOGLE_OAUTH_CLIENT_ID=your_google_client_id_here
VITE_GOOGLE_OAUTH_CLIENT_SECRET=your_google_client_secret_here
VITE_GOOGLE_REDIRECT_URI=http://localhost:5173/auth/google/callback

# -----------------------------------
# AI Services (Optional)
# -----------------------------------
# For AI-powered features (descriptions, auto-replies, supplier search)
VITE_AI_API_KEY=your_openai_or_claude_api_key_here
VITE_AI_PROVIDER=openai  # openai, claude, or gemini
VITE_AI_MODEL=gpt-4  # gpt-4, claude-3-opus, gemini-pro

# Supplier search API (if using custom service)
VITE_AI_SUPPLIER_SEARCH_ENDPOINT=https://api.yoursupplierservice.com

# -----------------------------------
# Feature Flags
# -----------------------------------
VITE_FEATURE_CALENDAR_AUTO_REMINDER=true
VITE_FEATURE_SHEETS_PREVIEW=true
VITE_FEATURE_ADS_MODULE=true
VITE_FEATURE_AI_ASSISTANT=false

# -----------------------------------
# API Configuration
# -----------------------------------
# Request retry settings
VITE_API_RETRY_ATTEMPTS=3
VITE_API_RETRY_DELAY=1000
VITE_API_TIMEOUT=30000

# Cache settings (in milliseconds)
VITE_CACHE_TTL=300000  # 5 minutes

# Rate limiting
VITE_RATE_LIMIT_REQUESTS=100
VITE_RATE_LIMIT_WINDOW=60000  # 1 minute

# -----------------------------------
# Development Settings
# -----------------------------------
# Enable debug logging
VITE_DEBUG=false

# Enable mock data even in LIVE mode (for testing)
VITE_FORCE_MOCK=false

# -----------------------------------
# Production Settings
# -----------------------------------
# Sentry DSN for error tracking (optional)
VITE_SENTRY_DSN=

# Google Analytics tracking ID (optional)
VITE_GA_TRACKING_ID=

# -----------------------------------
# Notes
# -----------------------------------
# 1. Copy this file to .env and fill in your actual values
# 2. Never commit .env to version control
# 3. For production, set VITE_APP_MODE=LIVE
# 4. All VITE_ prefixed variables are exposed to the browser
# 5. Keep API keys and secrets secure
# 6. Use environment-specific .env files (.env.development, .env.production)
